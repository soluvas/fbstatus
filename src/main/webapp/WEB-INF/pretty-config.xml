<pretty-config xmlns="http://ocpsoft.com/prettyfaces/3.3.3"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://ocpsoft.com/prettyfaces/3.3.3
						http://ocpsoft.com/xml/ns/prettyfaces/ocpsoft-pretty-faces-3.3.3.xsd">

	<url-mapping id="home">
		<pattern value="/" />
		<view-id value="/faces/front.xhtml" />
	</url-mapping>
	<url-mapping id="about">
		<pattern value="/about" />
		<view-id value="/faces/about.xhtml" />
	</url-mapping>
	<url-mapping id="contact">
		<pattern value="/contact" />
		<view-id value="/faces/contact.xhtml" />
	</url-mapping>

</pretty-config>